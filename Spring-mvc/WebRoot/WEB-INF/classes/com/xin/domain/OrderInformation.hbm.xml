<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.xin.domain">
	<class name="OrderInformation" table="orderinformation">
		<id name="ID" type="java.lang.Integer">
			<generator class="assigned"></generator>
		</id>
		<property name="OrderID" type="java.lang.Integer">
			<column name="OrderID" length="128" not-null="true" />
		</property>
		<property name="ItemID" type="java.lang.Integer">
			<column name="ItemID" length="128" not-null="true" />
		</property>
		<property name="ItemNumber" type="java.lang.Integer">
			<column name="ItemNumber" length="128" not-null="true" />
		</property>



	</class>
</hibernate-mapping>
